\nimport { useDispatch } from \"react-redux\";\nimport { filterStops, resetToOne } from \"../../redux/actions/filterStops\";\nimport { Form } from 'react-bootstrap';\nimport './Checkbox.css'\n\nconst Checkbox = ({ text, name, id, stops, all, isChecked }) => {\n\n    const [isShown, setIsShown] = useState(false);\n    const inp = useRef(null)\n\n    const dispatch = useDispatch();\n    const filteringByStops = value => dispatch(filterStops(value))\n\n    const checkboxHandler = event => {\n\n        if (event.target.checked) {\n            \n            filteringByStops(stops)\n        } else {\n\n            filteringByStops(stops)\n        }\n    }\n\n    useEffect(() => {\n        inp.current.checked = isChecked\n    }, [isChecked])\n\n    return (\n        <div className={`input-wrap ${isShown ? 'hovered' : ''}`} \n            onMouseEnter={() => setIsShown(true)} \n            onMouseLeave={() => setIsShown(false)}\n        >\n            <Form.Check\n                type=\"checkbox\"\n                label={ text }\n                name={ name }\n                id={ id }\n                onClick={ checkboxHandler }\n                ref={ inp }\n            />\n            {\n                isShown && (\n                    <p className=\"text-hover\"\n                       onClick={ () => { dispatch(resetToOne(stops)) } }\n                    >\n                        Только\n                    </p>\n                )\n            }\n        </div>\n    )\n}\n\nexport default Checkbox;","/home/smiss/Desktop/aviasales/src/redux/actions/GetTickets.js",[],"/home/smiss/Desktop/aviasales/src/components/TicketItem/TicketItem.js",[],"/home/smiss/Desktop/aviasales/src/redux/actions/switchCurrency.js",[],"/home/smiss/Desktop/aviasales/src/redux/types/currencyTypes.js",[],"/home/smiss/Desktop/aviasales/src/redux/types/getTickets.js",[],"/home/smiss/Desktop/aviasales/src/redux/types/filter.js",[],"/home/smiss/Desktop/aviasales/src/redux/actions/filterStops.js",[],"/h